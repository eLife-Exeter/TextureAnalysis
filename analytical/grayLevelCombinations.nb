Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"binToAvg", "[", 
    RowBox[{"b_", ",", "f_"}], "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"b", "\[Equal]", "0"}], ",", 
     RowBox[{"1", "-", "f"}], ",", "f"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"patternToAvg", "[", "p_", "]"}], ":=", 
   RowBox[{
    RowBox[{"binToAvg", "[", 
     RowBox[{
      RowBox[{"p", "[", 
       RowBox[{"[", 
        RowBox[{"1", ",", "1"}], "]"}], "]"}], ",", 
      RowBox[{"\[Phi]", "[", 
       RowBox[{"0", ",", "0"}], "]"}]}], "]"}], 
    RowBox[{"binToAvg", "[", 
     RowBox[{
      RowBox[{"p", "[", 
       RowBox[{"[", 
        RowBox[{"1", ",", "2"}], "]"}], "]"}], ",", 
      RowBox[{"\[Phi]", "[", 
       RowBox[{"1", ",", "0"}], "]"}]}], "]"}], 
    RowBox[{"binToAvg", "[", 
     RowBox[{
      RowBox[{"p", "[", 
       RowBox[{"[", 
        RowBox[{"2", ",", "1"}], "]"}], "]"}], ",", 
      RowBox[{"\[Phi]", "[", 
       RowBox[{"0", ",", "1"}], "]"}]}], "]"}], " ", 
    RowBox[{"binToAvg", "[", 
     RowBox[{
      RowBox[{"p", "[", 
