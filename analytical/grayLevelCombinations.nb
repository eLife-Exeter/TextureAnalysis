 RowBox[{
  RowBox[{
   RowBox[{"binToAvgSigned", "[", 
    RowBox[{"b_", ",", "f_"}], "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"b", "\[Equal]", "0"}], ",", 
     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{"(", 
       RowBox[{"1", "-", "f"}], ")"}]}], ",", 
